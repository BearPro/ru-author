                        <a href="#"><img class="img-svg" src="~/img/Social icons/Orange/vk.svg" /></a>
                    </div>
                </div>
                <div class="timer"></div>
                <a asp-action="Become"
                    asp-controller="Participant"
                    asp-route-guid="@Model.Guid"><button class="formButton">Участвовать</button></a>

                </div>
            <div class="share-icons">
                <a href="#"><img class="img-svg" src="~/img/Social icons/Orange/facebook.svg" /></a>
                <a href="#"><img class="img-svg" src="~/img/Social icons/Orange/google.svg" /></a>
